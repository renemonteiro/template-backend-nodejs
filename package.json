{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules  src/server.ts",
    "start": "node build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "knex: create Knexfile - init": "cd src/database && knex --knexfile knex init -x ts",
    "knex: create migration file": " cd src/database && knex --knexfile knex migrate:make migration_name -x ts",
    "knex: make migrate": "cd src/database/migrations && knex --knexfile knexfile.ts migrate:latest",
    "knex: create seed file": "cd src/database && knex --knexfile knexfile.ts seed:make seed_name",
    "knex: make seeds": "cd src/database/seeds && knex --knexfile knexfile.ts seed:run",
    "knex: rollback": "cd src/database && knex --knexfile knexfile.ts migrate:rollback",
    "knex: rollback--all": "cd src/database && knex --knexfile knexfile.ts migrate:rollback --all"
  },
  "keywords": [],
